{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.38.1",
    "@ant-design/pro-layout": "^6.25.1",
    "@umijs/preset-react": "1.8.23",
    "copy-to-clipboard": "^3.3.1",
    "react-hot-keys": "^2.7.0",
    "semver": "^7.3.5",
    "umi": "^3.5.17"
  },
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@umijs/test": "^3.5.17",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.4.2",
    "yorkie": "^2.0.0"
  }
}
