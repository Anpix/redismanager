{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.1",
    "antd": "4.5.3",
    "axios": "^0.19.2",
    "codemirror": "^5.52.2",
    "copy-to-clipboard": "^3.3.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-codemirror2": "^7.1.0",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-hot-keys": "^2.5.2",
    "umi": "^3.1.1",
    "vkbeautify": "^0.99.3",
    "yorkie": "^2.0.0"
  }
}
