{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "^1.x",
    "@umijs/test": "^3.3.7",
    "antd": "4.12.3",
    "axios": "0.21.1",
    "codemirror": "5.59.2",
    "copy-to-clipboard": "3.3.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.2",
    "react": "^16.x",
    "react-codemirror2": "7.2.1",
    "react-dom": "^16.x",
    "react-highlight-words": "0.16.0",
    "react-hot-keys": "2.6.2",
    "umi": "3.3.7",
    "vkbeautify": "0.99.3",
    "yorkie": "^2.0.0"
  }
}
